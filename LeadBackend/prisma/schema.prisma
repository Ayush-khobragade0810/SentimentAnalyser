generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model comments {
  id                Int       @id @default(autoincrement())
  stakeholder_name  String?   @db.VarChar(100)
  section_reference String?   @db.VarChar(50)
  comment           String?
  created_at        DateTime? @default(now()) @db.Timestamp(6)
}
